<% title "Delivery Presets" %>

<table>
  <tr>
    <th>Store</th>
    <th>Day Of Week</th>
  </tr>
  <% for delivery_preset in @delivery_presets %>
    <tr>
      <td><%=h delivery_preset.store_id %></td>
      <td><%=h delivery_preset.day_of_week %></td>
      <td><%= link_to "Show", delivery_preset %></td>
      <td><%= link_to "Edit", edit_delivery_preset_path(delivery_preset) %></td>
      <td><%= link_to "Destroy", delivery_preset, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Delivery Preset", new_delivery_preset_path %></p>
